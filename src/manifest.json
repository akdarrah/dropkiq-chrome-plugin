{
  "name": "Dropkiq for Chrome",
  "description": "Easily write Liquid expressions",
  "version": 1.01,
  "permissions": ["declarativeContent", "storage"],
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "new-tab-page",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+I",
        "mac": "Command+Shift+I"
      }
    }
  }
}
